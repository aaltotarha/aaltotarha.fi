{
  "public": false,
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Content-Security-Policy": "default-src 'none'; connect-src 'self'; font-src 'self'; img-src 'self' data:; manifest-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "Strict-Transport-Security": "max-age=63072000; includeSubdomains; preload",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
      },
      "dest": "/$1",
      "continue": true
    },
    {
      "src": "/*.html",
      "headers": {
        "Cache-Control": "public,max-age=0,must-revalidate,s-maxage=2592000,stale-while-revalidate"
      },
      "dest": "/$1"
    },
    {
      "src": "/build/(.*)",
      "headers": {
        "Cache-Control": "public,immutable,max-age=31536000,stale-while-revalidate"
      },
      "dest": "/build/$1",
      "continue": true
    },
    {
      "src": "/*.(jpg|png|svg|site.webmanifest)$",
      "headers": {
        "Cache-Control": "public,max-age=2592000,stale-while-revalidate"
      },
      "dest": "/$1"
    }
  ]
}